{
  "name": "Attendance",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "required": false
    },
    "Stats": [
      {
        "method": "stats",
        "endpoint": "/stats",
        "description": "Statistical information for Attendance registers.",
        "type": "model",
        "count": {
          "on": "createdAt",
          "by": "index"
        }
      }
    ]
  },
  "properties": {
    "uid": {
      "type": "string",
      "required": true
    },
    "name": {
      "type": "string"
    },
    "dateTime": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "employee": {
      "type": "belongsTo",
      "model": "Employee"
    }
  },
  "acls": [],
  "methods": {}
}
