{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins",
      "../node_modules/@mean-expert/loopback-stats-mixin/dist",
      "../node_modules/@mean-expert/loopback-component-realtime/dist/mixins",
      "../node_modules/loopback-ds-timestamp-mixin"
    ]
  },
  "User": {
    "dataSource": "db",
    "public": false
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongodb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongodb",
    "public": true
  },
  "Cut": {
    "dataSource": "mongodb",
    "public": true
  },
  "AppUser": {
    "dataSource": "mongodb",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "Attendance": {
    "dataSource": "mongodb",
    "public": true
  },
  "Employee": {
    "dataSource": "mongodb",
    "public": true
  },
  "Holiday": {
    "dataSource": "mongodb",
    "public": true
  },
  "OverTime": {
    "dataSource": "mongodb",
    "public": true
  },
  "Email": {
    "dataSource": "emailDataSource",
    "public": true
  },
  "pattern": {
    "dataSource": "mongodb",
    "public": true
  },
  "container": {
    "dataSource": "storage",
    "public": true
  },
  "spWorkingDay": {
    "dataSource": "mongodb",
    "public": true
  },
  "ImageFile": {
    "dataSource": "mongodb",
    "public": true
  },
  "target": {
    "dataSource": "mongodb",
    "public": true
  }
}
